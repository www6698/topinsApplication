<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:toolbox="clr-namespace:tpToolbox.Design.Themes;assembly=tpToolbox">
    <SolidColorBrush x:Key="IconBrush" Color="White" />
    <SolidColorBrush x:Key="IconInactiveBrush" Color="#66FFFFFF" />
    <SolidColorBrush x:Key="HeaderBackground" Color="#FF1F1F1F" />
    <SolidColorBrush x:Key="StatusBarBackground" Color="#FF424242" />
    <SolidColorBrush x:Key="StatusBarForeground" Color="White" />
    <SolidColorBrush x:Key="SystemButtonsForeground" Color="White" />
    <SolidColorBrush x:Key="SystemButtonsInactiveForeground" Color="#818183" />
    <SolidColorBrush x:Key="SystemButtonsMouseOverBackground" Color="#FF3D3D3D" />
    <SolidColorBrush x:Key="SystemButtonsMousePressedForeground" Color="White" />
    <SolidColorBrush x:Key="SystemButtonsMousePressedBackground" Color="#FF383838" />
    <SolidColorBrush x:Key="WindowForeground" Color="White" />
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF7160E8" />
    <SolidColorBrush x:Key="WindowInactiveBorderBrush" Color="#434346" />
    <SolidColorBrush x:Key="ToolBarGripper" Color="#656565" />
    <SolidColorBrush x:Key="ProjectNameForeground" Color="White" />
    <SolidColorBrush x:Key="ProjectNameInactiveForeground" Color="#818181" />
    <SolidColorBrush x:Key="ProjectNameBackground" Color="#FF424242" />
    <SolidColorBrush x:Key="ProjectNameInactiveBackground" Color="#FF1F1F1F" />
    <SolidColorBrush x:Key="ScrollViewerRectBackground" Color="#333333" />
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type toolbox:tpResourceKeys}, ResourceId=NormalBackgroundBrushKey}" Color="Transparent" />
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type toolbox:tpResourceKeys}, ResourceId=TopLevelHeaderMenuBackgroundKey}" Color="Transparent" />
</ResourceDictionary>