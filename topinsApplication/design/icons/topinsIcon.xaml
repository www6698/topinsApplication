<UserControl x:Class="topinsApplication.topinsIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:topinsApplication"
             mc:Ignorable="d">
    <Path x:Name="TOP_Menu" Data ="{DynamicResource SystemIcon}" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=UserControl}}" Stretch="Uniform"/>
</UserControl>
